   "source": [
    "## Clean the Service Column"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# inspect the 'institute_service' column\n",
    "combined['institute_service'].value_counts()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# We are going to ese vectorized string methods to extract the years of service from each pattern. \n",
    "# We start by changes the datatype to string\n",
    "service_strings = combined_updated['institute_service'].astype('str')\n",
    "# Splits values with dashes and saves first number in range\n",
    "service_strings = service_strings.str.split('-').str.get(0)\n",
    "# Use str.replace() for rows containing words\n",
    "service_strings = service_strings.str.replace('Less than 1 year', '0')\n",
    "service_strings = service_strings.str.replace('More than 20 years', '21')\n",
    "# Convert string values back floats\n",
    "combined_updated['institute_service'] = service_strings.astype('float').copy()\n",
    "# Confirm the change has been made correctly\n",
    "print(combined_updated['institute_service'].value_counts())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# We'll map these values according to the definitions below:\n",
    "# New: Less than 3 years at a company\n",
    "# Experienced: 3-6 years at a company\n",
    "# Established: 7-10 years at a company\n",
    "# Veteran: 11 or more years at a company\n",
    "\n",
    "# Create a function that converts floats to the keywords\n",
    "\n",
    "def experience(val):\n",
    "    if val < 3 :\n",
    "        return \"New\"\n",
    "    elif 3 <= val <= 6:\n",
    "        return \"Experienced\"\n",
    "    elif 7 <= val <= 10:\n",
    "        return \"Established\"\n",
    "    else:\n",
    "        return \"veteran\"\n",
    "\n",
    "# Apply the function to the column\n",
    "combined_updated['service_cat'] = combined_updated['institute_service'].apply(experience).copy()\n",
    "\n",
    "# Verify the new column has been created and the'institute_service' remains unchanged\n",
    "print(combined_updated['service_cat'].head(), '\\n', combined_updated['institute_service'].head())\n"
   ]
