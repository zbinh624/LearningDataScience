   "outputs": [],
   "source": [
    "# Next, we'll identify any employees who resigned because they were dissatisfied.\n",
    "# We define employees as such by the following columns.\n",
    "\n",
    "# Assigning tafe columns to be analyzed\n",
    "tafe_resignation_columns = ['Contributing Factors. Dissatisfaction', 'Contributing Factors. Job Dissatisfaction']\n",
